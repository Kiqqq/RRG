    <select id="selectLeafCold" resultType="cn.com.common.entity.dto.RiceGenesCategoriesPjDto">
        SELECT
        GENE_ID as geneId,
        SAMPLE as sample,
        RICE_VALUE as riceValue
        FROM LEAF_COLD
        WHERE sample IN
        <foreach item="item" collection="sampleList" open="(" separator="," close=")">
            #{item}
        </foreach>
    </select>

